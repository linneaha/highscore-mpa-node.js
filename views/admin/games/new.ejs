<form method="post">
  <h3>Nytt Spel</h3>

  <label for="title">Titel</label>
  <input type="text" id="title" name="title"/>

  <label for="desc">Beskrivning</label>
  <textarea id="desc" name="description" rows="10" cols="50"></textarea>

  <label for="image">Bild</label>
  <input type="file" id="image" name="imageUrl"/>

  <label for="genre">Genre</label>
  <select name="genre" id="genre">
    <option>Dynamiska options från databas</option>
  </select>

  <label for="releaseYear">Lanseringsår</label>
  <input type="number" id="releaseYear" name="releaseYear"/>

  <button>Lägg till</button>
</form>
